{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "es2017"
    ],
    "jsx": "preserve",
    "module": "esnext",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "typings/**/*.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ]
}